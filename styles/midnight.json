{
    "version": 2,
    "title": "Midnight",
    "styles": {
        "color": {
            "background": "#1b262c",
            "text": "#ffffff"
        },
        "blocks": {
            "core/paragraph": {
                "color": {
                    "background": "transparent",
                    "text": "#ffffff"
                },
                "spacing": {
                    "margin": "1.5rem auto",
                    "padding": 0
                },
                "typography": {
                    "fontStyle": "normal",
                    "fontSize": "18px",
                    "fontWeight": "400",
                    "lineHeight": "1.5"
                },
                "border": {
                    "color": "rgba(0,0,0,0)",
                    "radius": 0,
                    "style": "none",
                    "width": 0
                }
            },

            "core/code": {
                "color": {
                    "background": "var(--wp--custom--color--dark-button-bg)",
                    "text": "var(--wp--custom--color--dark-button-text)"
                }
            }
        }
    },
    "settings": {
        "custom": {
            "color": {
                "scheme": "midnight",
                "link": "#cccccc",
                "link-hover": "#ffffff"
            }
        }
    },
    "elements": {
        "link": {
            "color": {
                "text": "#cccccc"
            },
            ":hover": {
                "color": {
                    "text": "#ffffff"
                }
            }
        }
    }
}